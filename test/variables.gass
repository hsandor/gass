
$col-black: #012345
$col-sky: rgb(1,$col-black,2)
$apple:apple

nav
	$something: anything
	ul
		everything: $something
		background-color: $col-black

	li
		$apple:pear
		background-color:$col-sky
		everything: $something
		apple:$apple

	a
		apple:$apple

.users #userTab,
.posts #postTab
  everything: $something
  background-color: $col-sky
